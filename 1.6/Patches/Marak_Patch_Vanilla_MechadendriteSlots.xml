<?xml version="1.0" encoding="utf-8" ?>
 <Patch>
    <!-- These add the mechadendrite slots onto humanoid pawns -->
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
        <order>Append</order>
        <value>
            <li>
                <def>Mechadendrite_Slot</def>
                <customLabel>mechadendrite slot 1</customLabel>
                <coverage>0.000</coverage>
                <depth>Outside</depth>
                <groups>
                    <li>Torso</li>
                </groups>
            </li>
        </value>
    </Operation>

