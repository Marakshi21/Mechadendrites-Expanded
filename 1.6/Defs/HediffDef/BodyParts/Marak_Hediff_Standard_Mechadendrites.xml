            <color>(189,169,118)</color>
        </graphicData>
        <tradeTags>
            <li>Bionic</li>
        </tradeTags>
        <techHediffsTags>
            <li>Advanced</li>
        </techHediffsTags>
        <statBases>
            <WorkToMake>18000</WorkToMake>
        </statBases>
        <recipeMaker>
            <unfinishedThingDef>UnfinishedHealthItemBionic</unfinishedThingDef>
            <skillRequirements>
