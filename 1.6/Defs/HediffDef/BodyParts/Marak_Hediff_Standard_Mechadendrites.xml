    <!-- high Mechadendrite -->
    <HediffDef ParentName="AddedBodyPartBase">
        <defName>Marak_StandardMechadendrite</defName>
        <label>high general mechadendrite</label>
        <labelNoun>a high general mechadendrite</labelNoun>
        <description>An installed high general mechadendrite. A unique plasteel-steel blend and advanced circuitry grant it exceptional agility and utility.</description>
        <descriptionHyperlinks><ThingDef>Marak_StandardMechadendrite</ThingDef></descriptionHyperlinks>
        <spawnThingOnRemoved>Marak_StandardMechadendrite</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
        </addedPartProps>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.05</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>

    <ThingDef ParentName="BodyPartStandardMechadendriteBase">
        <defName>Marak_StandardMechadendrite</defName>
        <label>high general mechadendrite</label>
        <description>Fabricated using precision manufacturing, a unique blend of plasteel and steel give ample protection and agility while keeping costs down. While internal circuitry and nerve-link gels grant direct control over the high general mechadendrite. All while mini-servos provide fine motor function. It is superior to any previous mechadendrite in every way.</description>
        <descriptionHyperlinks><RecipeDef>Marak_InstallStandardMechadendrite</RecipeDef></descriptionHyperlinks>
        <costList>
            <Plasteel>10</Plasteel>
            <ComponentSpacer>1</ComponentSpacer>
            <Steel>5</Steel>
        </costList>
        <thingSetMakerTags><li>RewardStandardMidFreq</li></thingSetMakerTags>
        <statBases>
            <Mass>2.5</Mass>
        </statBases>
    </ThingDef>

    <RecipeDef ParentName="SurgeryInstallBodyPartMechadendriteBase">
        <defName>Marak_InstallStandardMechadendrite</defName>
        <label>install high general mechadendrite</label>
        <description>Install a high general mechadendrite.</description>
        <descriptionHyperlinks>
            <ThingDef>Marak_StandardMechadendrite</ThingDef>
            <HediffDef>Marak_StandardMechadendrite</HediffDef>
        </descriptionHyperlinks>
        <jobString>Installing a high general mechadendrite.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Marak_StandardMechadendrite</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Marak_StandardMechadendrite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Mechadendrite_Slot</li>
        </appliedOnFixedBodyParts>
        <addsHediff>Marak_StandardMechadendrite</addsHediff>
    </RecipeDef>

    <RecipeDef ParentName="SurgeryRemoveImplantBase">
        <defName>Marak_RemoveStandardMechadendrite</defName>
