    <!-- Base for high Mechadendrites -->
    <ThingDef Name="BodyPartStandardMechadendriteBase" ParentName="BodyPartBase" Abstract="True">
        <techLevel>Spacer</techLevel>
        <thingCategories>
            <li>BodyPartsBionic</li>
        </thingCategories>
        <graphicData>
            <color>(189,169,118)</color>
            <texPath>Things/Item/Health/HealthItem</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>0.8</drawSize>
        </graphicData>
        <tradeTags>
            <li>Bionic</li>
        </tradeTags>
        <techHediffsTags>
            <li>Advanced</li>
        </techHediffsTags>
        <statBases>
            <WorkToMake>15000</WorkToMake>
            <Mass>2.5</Mass>
        </statBases>
        <recipeMaker>
            <unfinishedThingDef>UnfinishedHealthItemBionic</unfinishedThingDef>
            <skillRequirements>
                <Crafting>8</Crafting>
            </skillRequirements>
            <researchPrerequisite>Marak_StandardMechadendrites</researchPrerequisite>
            <recipeUsers>
                <li>FabricationBench</li>
            </recipeUsers>
            <displayPriority>300</displayPriority>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
        </recipeMaker>
    </ThingDef>

    <!-- high Mechadendrite -->
    <HediffDef ParentName="AddedBodyPartBase">
        <defName>Marak_StandardMechadendrite</defName>
        <label>high general mechadendrite</label>
        <labelNoun>a high general mechadendrite</labelNoun>
        <description>An installed high general mechadendrite. A unique plasteel-steel blend and advanced circuitry grant it exceptional agility and utility.</description>
        <descriptionHyperlinks><ThingDef>Marak_StandardMechadendrite</ThingDef></descriptionHyperlinks>
        <spawnThingOnRemoved>Marak_StandardMechadendrite</spawnThingOnRemoved>
        <alwaysAllowMothball>True</alwaysAllowMothball>
        <alwaysAllowMothballCached>True</alwaysAllowMothballCached>
        <addedPartProps>
            <solid>true</solid>
        </addedPartProps>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.075</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>

    <ThingDef ParentName="BodyPartStandardMechadendriteBase">
        <defName>Marak_StandardMechadendrite</defName>
        <label>high general mechadendrite</label>
        <description>Forged with a unique blend of plasteel and steel to reduce costs and precision fabrication, the high general mechadendrite surpasses all that comes before it.</description>
        <descriptionHyperlinks><RecipeDef>Marak_InstallStandardMechadendrite</RecipeDef></descriptionHyperlinks>
        <costList>
            <Plasteel>10</Plasteel>
            <ComponentSpacer>1</ComponentSpacer>
            <ComponentIndustrial>1</ComponentIndustrial>
            <Steel>10</Steel>
        </costList>
        <statBases>
            <Mass>2.5</Mass>
        </statBases>
    </ThingDef>

    <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
        <defName>Marak_InstallStandardMechadendrite</defName>
        <label>install high general mechadendrite</label>
        <description>Install a high general mechadendrite.</description>
        <descriptionHyperlinks>
            <ThingDef>Marak_StandardMechadendrite</ThingDef>
            <HediffDef>Marak_StandardMechadendrite</HediffDef>
        </descriptionHyperlinks>
        <jobString>Installing a high general mechadendrite.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Marak_StandardMechadendrite</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Marak_StandardMechadendrite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Mechadendrite_Slot</li>
        </appliedOnFixedBodyParts>
        <addsHediff>Marak_StandardMechadendrite</addsHediff>
        <skillRequirements>
            <Medicine>12</Medicine>
        </skillRequirements>
    </RecipeDef>

    <RecipeDef ParentName="SurgeryRemoveImplantBase">
        <defName>Marak_RemoveStandardMechadendrite</defName>
