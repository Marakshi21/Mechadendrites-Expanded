    <!-- Upper Left Basic Mechadendrite -->
    <RecipeDef ParentName="SurgeryInstallImplantBase">
        <defName>InstallUpperLeftBasicMechadendrite</defName>
        <label>install basic mechadendrite (upper left)</label>
        <description>Install basic mechadendrite (upper left).</description>
        <descriptionHyperlinks>
            <ThingDef>BasicMechadendrite</ThingDef>
            <HediffDef>ULBasicMechadendrite</HediffDef>
        </descriptionHyperlinks>
        <jobString>Installing basic Mechadendrite (upper left).</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>BasicMechadendrite</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>BasicMechadendrite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Torso</li>
        </appliedOnFixedBodyParts>
        <addsHediff>ULBasicMechadendrite</addsHediff>
        <incompatibleWithHediffTags><li>ULSpacerMechadendrite</li></incompatibleWithHediffTags>
        <researchPrerequisite>Marak_AdvancedMechadendriteImplantation</researchPrerequisite>
    </RecipeDef>

