<Defs>
    
    <!-- Base: Simple Mechadendrites -->
    <ThingDef Name="BodyPartMechadendriteBase" ParentName="BodyPartBase" Abstract="True">
        <techLevel>Industrial</techLevel>
        <thingCategories>
            <li>BodyPartsProsthetic</li>
        </thingCategories>
        <graphicData>
            <texPath>Things/Item/Health/HealthItem</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>0.80</drawSize>
            <color>(154,124,104)</color>
        </graphicData>
        <tradeTags>
            <li>Prosthetic</li>
        </tradeTags>
        <techHediffsTags>
            <li>Simple</li>
        </techHediffsTags>
        <statBases>
            <WorkToMake>15000</WorkToMake>
        </statBases>
        <recipeMaker>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
            <unfinishedThingDef>UnfinishedHealthItemBionic</unfinishedThingDef>
            <skillRequirements>
                <Crafting>6</Crafting>
            </skillRequirements>
            <researchPrerequisite>Marak_IndustrialMechadendrites</researchPrerequisite>
            <recipeUsers>
                <li>TableMachining</li>
            </recipeUsers>
            <displayPriority>450</displayPriority>
        </recipeMaker>
    </ThingDef>

