    <!-- INDUSTRIAL MECHADENDRITES -->

    <!-- Upper Left Basic Mechadendrite -->
    <HediffDef ParentName="ImplantHediffBase">
        <defName>ULBasicMechadendrite</defName>
        <label>Basic Mechadendrite (upper Left)</label> <!-- Find better name -->
        <labelNoun>an upper left basic mechadendrite</labelNoun>
        <description>A small and rudimentary limb attached onto this individual's back boosting manipulation.</description>
        <descriptionHyperlinks><ThingDef>BasicMechadendrite</ThingDef></descriptionHyperlinks>
        <spawnThingOnRemoved>BasicMechadendrite</spawnThingOnRemoved>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.025</offset>
                    </li>
                </capMods>
            </li>
        </stages>
        <duplicationAllowed>false</duplicationAllowed>
        <tags><li>Mechadendrite</li></tags>
